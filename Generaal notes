# range -–≥–µ–Ω–µ—Ä–∞—Ç–æ—Ä —á–∏—Å–µ–ª


import random

random_box = ''
for i in range(1000000):
  random_box += str(random.randint(1, 3))

print("1=", random_box.count('1'))
print("2=", random_box.count('2'))
print("3=", random_box.count('3'))

